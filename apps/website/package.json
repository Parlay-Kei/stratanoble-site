{
  "name": "@strata-noble/website",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env PORT=8080 next dev",
    "dev:fast": "cross-env PORT=8080 NODE_ENV=development next dev --turbo",
    "dev:8080": "cross-env PORT=8080 next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "changelog": "node scripts/generate-changelog.js",
    "version:patch": "npm version patch && npm run changelog",
    "version:minor": "npm version minor && npm run changelog",
    "version:major": "npm version major && npm run changelog",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:diff": "supabase db diff -f",
    "db:push": "supabase db push",
    "db:combine": "node scripts/combine-migrations.js",
    "db:validate": "node scripts/validate-migrations.js",
    "db:new": "supabase migration new",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:security": "jest --testNamePattern=\"XSS|security|SafeHTML|Security Test Suite|Core Security Test Suite\"",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
